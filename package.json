{
  "name": "specialty-slurper",
  "version": "0.0.2",
  "description": "Tool to extract winners from AKC show results (for Basenji Specialties in particular)",
  "author": "Jared Reisinger",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/JaredReisinger/specialty-slurper#readme",
  "repository": "github:JaredReisinger/specialty-slurper",
  "bugs": "https://github.com/JaredReisinger/specialty-slurper/issues",
  "type": "module",
  "main": "src/index.js",
  "bin": "src/cli.js",
  "scripts": {
    "start": "node ./src/cli.js",
    "test": "ava --verbose",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "c8 --all --src src --exclude cli.js --exclude '**/*.test.js' npm run test",
    "lint": "eslint --ext js,mjs --ignore-path .prettierignore ."
  },
  "dependencies": {
    "chalk": "^2.3.0",
    "commander": "^2.13.0",
    "css-select": "^1.2.0",
    "htmlparser2": "^3.9.0",
    "progress": "^1.1.8"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "ava": "^3.11.1",
    "c8": "^7.3.0",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "semantic-release": "^17.1.1",
    "testdouble": "^3.16.1"
  },
  "ava": {
    "nodeArguments": [
      "--loader=testdouble"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && npm run lint",
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
